<?xml version="1.0" encoding="UTF-8"?>
<form className="com.mitian.airad.web.form.AiradDevmemberForm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="validate-schema.xsd">
     <fields>
        <fieldRule name="email">
            <pattern validator="required">
                <errorTipZH>请输入电子邮箱！</errorTipZH>
            </pattern>
            <pattern validator="email">
                <errorTipZH>电子邮箱格式错误</errorTipZH>
            </pattern>
        </fieldRule>
	</fields>
    <actions>
        <xcase name="doDevregister">
            <field name="email" />
        </xcase>
    </actions>
</form>